<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE xml>
<configuration monitorInterval="3">

	<appender>
		<Console name="OUT" target="SYSTEM_OUT">
			<Patternlayout pattern="%d{ABSOLUTE} [%-5p] (%F:%L) - %m%n" />
		</Console>
	</appender>
	<loggers>
		<root level="warn">
			<appender-ref ref="OUT" />
		</root>
		<logger name="log">
			<appender-ref ref="OUT" />
		</logger>
	</loggers>
</configuration>